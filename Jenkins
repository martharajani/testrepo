    stage('Submit Stack'){
      steps{
         sh "aws cloudformation create-stack --stack-name rajanicalculator --template-body file://package.yml --capabilities CAPABILITY_AUTO_EXPAND"
        #sh "aws cloudformation create-stack --stack-name SamTemplate --template-body file://rajani.yaml"
      }
    }
  }
  
}
